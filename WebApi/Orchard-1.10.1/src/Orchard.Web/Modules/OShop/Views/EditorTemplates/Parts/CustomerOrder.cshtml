@using OShop.Models
@using Orchard.Utility.Extensions
@{
    Style.Require("OShopAdmin");
    CustomerOrderPart part = Model;
}

@if (part.Customer != null) {
    <fieldset class="@part.PartDefinition.Name.HtmlClassify()">
        <legend>@T("Customer account")</legend>
        @Html.ItemAdminLink(part.Customer)
    </fieldset>
}
