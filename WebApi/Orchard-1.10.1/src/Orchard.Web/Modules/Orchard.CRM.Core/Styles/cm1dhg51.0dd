.admin-cancel {
    background-color: #6a7b42;
    border: 1px solid #487328;
    color: #fff;
    cursor: pointer;
    display: inline-block;
    font: 12px @label-font-family, Helvetica, sans-serif;
    padding: 5px 14px 3px 14px;
    text-align: center;
    text-decoration: none;
    text-shadow: rgba(40, 53, 9, 0.2) 0px 0px 1px;
    -webkit-box-shadow: inset 0px 0px 1px #ffffff, 1px 1px 1px rgba(102, 102, 102, 0.2);
    -moz-box-shadow: inset 0px 0px 1px #ffffff, 1px 1px 1px rgba(102, 102, 102, 0.2);
    box-shadow: inset 0px 0px 1px #ffffff, 1px 1px 1px rgba(102, 102, 102, 0.2);
    filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=0, startColorstr='#ff9bb36c', endColorstr='#ff809f43');
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    height: 16px;
}

    .admin-cancel:hover {
        background-color: #809f43;
        color: #eefcec;
    }

