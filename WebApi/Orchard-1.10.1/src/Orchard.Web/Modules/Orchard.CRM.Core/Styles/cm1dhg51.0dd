.popup-host {
    position: relative;
    display: inline-block;
    line-height: 17px;
}

    .popup-host span {
        overflow: hidden;
        display: inline-block;
        height: 20px;
    }

    .popup-host:hover span {
        background-image: @backgound-image-downTriangle-hover;
        background-size: 8px 8px;
        display: inline-block;
        width: 100%;
        background-position: right center;
        background-repeat: no-repeat;
        line-height: 17px;
    }

.line-property {
    color: gray;
    border-style: inset;
    border-width: 0.5px;
}

.popup-host .popup-menu-box {
    display: none;
    position: absolute;
    top: 21px;
    left: 0px;
    background-color: white;
    z-index: 100;
}

.popup-host .popup-menu {
    display: none;
    position: absolute;
    top: 21px;
    left: 0px;
    background-color: white;
    z-index: 100;
}

.popup-menu-box {
    width: 100%;
