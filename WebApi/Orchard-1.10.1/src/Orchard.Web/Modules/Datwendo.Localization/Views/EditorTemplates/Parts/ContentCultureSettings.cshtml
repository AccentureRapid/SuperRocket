@model Datwendo.Localization.Models.ContentCultureSettingsPart

<fieldset>
    <legend>@T("Content Culture Selector")</legend>
    <div>
        @Html.EditorFor(m => m.Enabled)
        <label for="@Html.FieldIdFor(m => m.Enabled)" class="forcheckbox">@T("Enable Localization by Content")</label>
        @Html.ValidationMessageFor(m => m.Enabled, "*")
    </div>
    <div>
        @Html.LabelFor(m => m.Priority, @T("Priority"))
        @Html.EditorFor(m => m.Priority)
    </div>
</fieldset>